[user]
	name = chaverman
	email = chaverman@zillowgroup.com
[alias]
	alias = !git config --get-regexp 'alias.*'
	amend-quick = commit -a --amend --no-edit
    cdup = !cd $(git rev-parse --show-cdup)
	co = checkout
	diffhead = diff HEAD^ HEAD
	ft = !git fetch && git status
	lg = log
    mkmaster =  checkout -b master origin/master
	st = status
	upstream = branch --set-upstream-to
[mergetool]
	keepBackup = true
[color]
	ui = true
[core]
	excludesfile = /Users/chaverman/.gitrc/gitignore_global
	pager = less
	ignorecase = false
; May be necessary?
;[diff]
;    external = git-diff
[pull]
    rebase = true
	ff = only
[merge]
	conflictstyle = diff3
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = current
